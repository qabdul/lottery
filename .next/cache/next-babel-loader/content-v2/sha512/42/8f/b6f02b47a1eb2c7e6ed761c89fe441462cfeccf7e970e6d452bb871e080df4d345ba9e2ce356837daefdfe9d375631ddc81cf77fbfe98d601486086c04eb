 from \"../etherium/Campaign\";\r\nimport web3 from \"../etherium/web3\";\r\nimport { Router } from \"../routes\";\r\nimport MessageLoader from \"./MessageLoader\";\r\n\r\nconst Contribute = (props) => {\r\n  const [value, setValue] = useState();\r\n  const [data, setData] = useState(\"\");\r\n  const [load, setLoad] = useState(false);\r\n  const campaign = Campaign(props.address);\r\n  const handleChange = (e) => {\r\n    const { value } = e.target;\r\n    if (!isNaN(value)) {\r\n      console.log(value);\r\n      setValue(value);\r\n    }\r\n  };\r\n\r\n  const onSubmit = async () => {\r\n    try {\r\n      setLoad(true);\r\n      setData(\"\");\r\n      const accounts = await web3.eth.getAccounts();\r\n      const result = await campaign.methods.contribute().send({\r\n        from: accounts[0],\r\n        value: value,\r\n      });\r\n      Router.replaceRoute(`/campaigns/${props.address}`);\r\n      setLoad(false);\r\n    } catch (error) {\r\n      setLoad(false);\r\n      setData(error.message);\r\n      console.log(error);\r\n    }\r\n  };\r\n  return (\r\n    <>\r\n      <Form error={!!data}>\r\n        <Form.Field>\r\n          <label>Contribute to this Campaign</label>\r\n          <Input\r\n            label=\"wei\"\r\n            labelPosition=\"right\"\r\n            onChange={(e) => handleChange(e)}\r\n            value={value}\r\n            type=\"number\"\r\n          />\r\n        </Form.Field>\r\n        <Message error content={data} header=\"OOPS!\" />\r\n\r\n        <Button floated=\"right\" loading={load} onClick={onSubmit} primary>\r\n          Contribute!\r\n        </Button>\r\n      </Form>\r\n      <Grid.Row></Grid.Row>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default Contribute;\r\n"]},"metadata":{},"sourceType":"module"}